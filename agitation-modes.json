  "schema": {
    "mode": {
      "name": "string - Display name of the mode",
      "localizedNameKey": "string - Localization key for the name",
      "description": "string - Description of the agitation pattern",
      "isBuiltIn": "boolean - Whether this is a built-in mode",
      "rules": "array - Array of agitation rules"
    },
    "rule": {
      "id": "string - Unique identifier for the rule",
      "priority": "integer - Rule priority (higher = more important)",
      "condition": "object - When this rule applies",
      "action": "string - Type of agitation action",
      "parameters": "object - Action-specific parameters"
    },
    "condition": {
      "type": "string - Type of condition from condition_types enum",
      "values": "array of integers - Condition-specific values"
    },
    "parameters": {
      "agitation_seconds": "integer - Duration of agitation in seconds",
      "rest_seconds": "integer - Duration of rest in seconds",
      "rotations": "integer - Number of rotations",
      "interval_seconds": "integer - Interval for periodic actions"
    }
  }
